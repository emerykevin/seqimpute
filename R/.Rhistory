library(microbenchmark)
n <- 1000
k <- 20
mat_list <- lapply(1:k, function(i) matrix(i, n, 1))
# cbind in loop
bench1 <- function() {
res <- mat_list[[1]]
for (i in 2:k) {
res <- cbind(res, mat_list[[i]])
}
res
}
# preallocate and fill
bench2 <- function() {
res <- matrix(NA, n, k)
for (i in 1:k) {
res[, i] <- mat_list[[i]][, 1]
}
res
}
microbenchmark(bench1(), bench2(), times = 100)
